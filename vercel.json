{
  "version": 2,
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": "apps/seller-panel/.next",
  "builds": [
    {
      "src": "apps/seller-panel/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "apps/admin-panel/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/seller-panel/(.*)",
      "dest": "apps/seller-panel/$1"
    },
    {
      "src": "/admin-panel/(.*)",
      "dest": "apps/admin-panel/$1"
    }
  ]
} 